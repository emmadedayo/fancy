{
  "name": "fancy",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f  -d ./src/libs/db/typeOrm.config.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "run:migrations": "npm run typeorm migration:run -- -d ./src/libs/db/typeOrm.config.ts",
    "generate:migration": "npm run typeorm -- -d ./src/libs/db/typeOrm.config.ts migration:generate ./src/libs/db/migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/libs/db/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/libs/db/typeOrm.config.ts migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.9",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.9",
    "aws-sdk": "^2.1637.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.9",
    "cache-manager": "^5.6.1",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^5.1.1",
    "chai": "^5.1.1",
    "class-transformer": "^0.5.1",
